<h1>Posts</h1>

<div class="col-md-4">
  <ul class="list-unstyled">
    {{#each model as |post|}}
      <li>
        {{#link-to 'post' post.id}}
          ({{post.votes}}) {{post.title}}
        {{/link-to}}
        by {{post.author.name}}
      </li>
    {{/each}}
  </ul>
</div>

<div class="col-md-8">
  {{outlet}}
</div>